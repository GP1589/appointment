generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model AppointmentPE {
  id             Int      @id @default(autoincrement())
  insuredId      String   @db.VarChar(10)
  scheduleId     Int
  centerId       Int
  specialtyId    Int
  medicId        Int
  appointmentDate DateTime
  createdAt      DateTime @default(now())
}

model AppointmentCL {
  id             Int      @id @default(autoincrement())
  insuredId      String   @db.VarChar(10)
  scheduleId     Int
  centerId       Int
  specialtyId    Int
  medicId        Int
  appointmentDate DateTime
  createdAt      DateTime @default(now())
}
